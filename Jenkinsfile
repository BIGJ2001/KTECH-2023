pipeline {
  agent any
  stages {
    stage('Build'){
      steps {
       echo "build step"
       sleep 10
     }
   }
    stage('test'){
      steps {
       echo "teststep"
       sleep 10
     }
   }
    stage('deploy'){
      steps {
       echo "deploystep"
       sleep 10
     }
   }
    stage('docker'){
      steps {
       echo "image step"
       sleep 10
     }
   }
  }
}
